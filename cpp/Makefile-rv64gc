CPP=/usr/bin/riscv64-linux-gnu-g++

all: gather_load scatter_store vadd_va_vb

gather_load: gather_load.cpp
	$(CPP) gather_load.cpp -O3 -march=rv64gc -static -o gather_load.riscv

scatter_store: scatter_store.cpp
	$(CPP) scatter_store.cpp -O3 -march=rv64gc -static -o scatter_store.riscv

vadd_va_vb: vadd_va_vb.cpp
	$(CPP) vadd_va_vb.cpp -O3 -march=rv64gc -static -o vadd_va_vb.riscv

clean:
	rm -rf *.rvv *.o
